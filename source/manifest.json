{
  "manifest_version": 2,

  "name": "Atbash secure file sender",
  "description": "Send files with end-to-end encryption from inside your Gmail",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "declarativeContent",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
